/* pages/aftervotetext/aftervotetext.wxss */
.twoDT{
    font-size: smaller ;
}

.moreInfo{
    margin: 20px 0px;
    font-size: 14px;
    text-align: center;
    font-weight: bolder;
    /* color:coral; */
}

.tab{
    display: flex;
    flex-flow: column;
}
.rankImage{
    height: 100px;
    text-align:center
}

.rankListNote{
    font-weight: bolder;
    font-size: 14px;
    color:red;
    display: flex;
    flex-flow: row;
    justify-content: center;
    margin: 8px 0px;
}

.rank{
    margin: 0px 10px 0px 15px;
    font-weight: bolder;
}

.content{
    margin-right: 30rpx;
    width:550rpx;
    display: inline-block;
    /* 解决设置inline-block后与行内其他元素不在同一水平线的问题 */
    vertical-align:top;
    white-space:nowrap;
    /* 超出隐藏 */
    overflow:hidden;
    /*超出显示为省略号*/
    text-overflow:ellipsis;
}

.num{
    position: absolute;
    right:10px;
    text-align:right
}


.twoButtons{
    display: flex;
    flex-flow: column;
    align-items: center;
}

._button{
   margin: 5px 0;
}

