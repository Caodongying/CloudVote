.cover{
    width:100%;
    height:136px;
}

.voteTitle{
    text-align: center;
    margin:5px 0px;
    color:coral;
    font-weight: bolder;  
}

.timeInfo{
    background-color: rgb(230, 230, 230);
    margin:10px 20px;
    display: flex;
    flex-flow: column;
    text-align: center;
}

.timetop-Info{
    
}

.totalInfo{
    display: flex;
    flex-flow: row;
    text-align: center;
    margin:15px 5px 0px 5px;
}

.left-Info{
    display: flex;
    flex-flow: column;
    margin-left: 30rpx;
    float: left;
    width:375rpx;
}

.right-Info{
    display: flex;
    flex-flow: column;
    margin-right: 30rpx;
    float: right;
    width:375rpx;
}

.info-num{
